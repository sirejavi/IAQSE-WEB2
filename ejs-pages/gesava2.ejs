<style id="gesava2-doc-css">
    .iaqse-contents {
        background: whitesmoke;
        border: 1px solid lightblue;
        box-shadow: 2px 2px lightgray;
        margin: 30px;
        padding: 15px;
        width: 98%;
    }
    .iaqse-section {
        width: 98%;
        border-bottom: 1px solid lightblue;
        margin: 60px 0 30px 0;
    }
    .img-center {
        max-width: 600px !important;
        margin: 30px auto !important;
    }
    .video-center {
        max-width: 800px !important; 
        margin: 30px auto !important;
    }
    .md-center {
        padding:5px;
        width: 70%;
        margin:auto;
    }
    @media print {
        body {
            margin:50px 25px 10px 25px;
        }
    }
</style>
<!--inject views here-->
<div ng-view id="routedContent"></div>

<script>
    function printDocument(divName, title) {
            var printContents = document.getElementById(divName).innerHTML;
            var cssContents = document.getElementById('gesava2-doc-css').innerHTML;
            var popupWin = window.open('', '_blank', 'width=800,height=600'); 
            popupWin.document.open();
            var stitle = "Ajuda Gesava2";
            if(title) {
                stitle += " - " + title;
            }
            popupWin.document.write('<html><head><title>'+stitle+'</title><link rel="stylesheet" type="text/css" href="iaqse_bundle.min.css" /><style>'+
               cssContents +'</style></head><body onload="window.print()">' + printContents + '</body></html>');
            popupWin.document.close();
    }
    app.controller("mainController", ["$scope", "httpService", function($scope, httpService){
        httpService.hit(location.href, "PAGE");
    }]);
</script>